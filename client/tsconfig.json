{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"strict": true,
		"noImplicitReturns": true,
		"typeRoots": ["node_modules/@types"],
		"noImplicitAny": true,
		"module": "es6",
		"moduleResolution": "node",
		"target": "es5",
		"allowJs": true,
		"jsx": "react"
	},
	"include": ["./src/**/*"]
}
